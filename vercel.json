{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php@0.8.0",
      "config": {
        "phpIni": {
          "memory_limit": "256M",
          "display_errors": "On",
          "log_errors": "On"
        }
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(css|js|build|images|fonts)/(.*)", "dest": "public/$1/$2" },
    { "src": "/(.*)\\.(.+)", "dest": "public/$1.$2" },
    { "src": "/(.*)", "dest": "api/index.php" }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://intan-deco1-fj3c.vercel.app",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
